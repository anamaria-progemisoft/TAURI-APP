{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "tauri-app",
    "version": "0.0.74"
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.github.com/anamaria-progemisoft/b9b7762da158272510b15824f01e55f6/raw"
        
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDM0N0MzM0QwN0NBMUJBQkUKUldTK3VxRjgwRE44TkZlZXFhWGFsbS9WMFV1ZnNQK0RHay9WR2o0Y0NwZXdsNGtLWENFWFpaSjMK"
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "identifier",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-app",
        "width": 800,
        "height": 600
      }
    ]
  }
}